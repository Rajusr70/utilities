echo '{"DEVICE":[{"G":"0","V":0,"D":999,"DA":"FFFFFF"}]}' > /dev/ttyO1
cp /opt/utilities/etc/fstab /etc/
cp /opt/utilities/etc/motd.tail /etc/
cp /opt/utilities/init/dmesg.conf /etc/init/
cp /opt/utilities/etc/rsyslog.d/50-default.conf /etc/rsyslog.d/
cp /opt/utilities/etc/update-motd.d/10-help-text /etc/update-motd.d/

echo '0.5' > /opt/utilities/sys_version
touch /etc/opt/ninja/.has_updated_system
sync